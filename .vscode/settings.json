{
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "files.eol": "\n",
  "eslint.packageManager": "yarn",
  "eslint.validate": [
      "javascript",
      "vue",
  ],
  "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
  },
  "eslint.run": "onSave",
  "npm.packageManager": "yarn"
}